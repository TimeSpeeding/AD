<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
<item android:state_pressed="true">
    <shape>
        <gradient android:startColor="#CC000000" android:endColor="#CC000000" android:angle="180"/>
        <stroke android:width="1dp" android:color="#CC000000"/>
        <!--<corners android:radius="10dp"/>-->
    </shape>
</item>
<item>
    <shape>
        <gradient android:startColor="#CC000000" android:endColor="#CC000000" android:angle="270"/>
        <stroke android:width="1dp" android:color="#CC000000"/>
        <!--<corners android:radius="10dp"/>-->
    </shape>
</item>
</selector>