
@{
    ViewBag.Title = "Index";
    if (Session["user"] == null)
    {
        Response.Redirect("~/home/login");
    }
    var user = (Group13SSIS.Models.User)Session["user"];
}

<h2>Index</h2>

@if (user != null)
{
    <h4>Hello, @user.Name</h4>
}

<div style="padding:10px 100px 0;">
    <img src="~/Content/image/LU.jpg" height="470" width="900" opacity="50%" ; />
</div>
